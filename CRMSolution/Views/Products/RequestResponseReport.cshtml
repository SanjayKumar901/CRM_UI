@{
    ViewData["Title"] = "RequestaAndResponseReport";
}
<script src="https://centralapi.stariglocal.com/JsMaster/CRMJS/js/Products/RequestResponseReport.js"></script>
@*<script src="~/js/Products/RequestResponseReport.js"></script>*@
<div class="page-wrapper ng-scope mappedUser open_Main" ng-controller="ReqResRep">
    <div class="container">
        <section class="mngUserfirst">
            <div class="row">
                <div class="col-md-6">
                    <h3>Request Response Report</h3>
                </div>
            </div>
        </section>
        <div class="row">
            <div class="col-md-12 PadRight mb-3 mt-3 text-right">
                <div id="ReqResCal" class="pull-right theFilters theFiltersClndr">
                    <span class="DateRangeShow DateRangeShowLast1"></span>
                    <i class="fas fa-calendar-alt"></i><b class="caret"></b>
                </div>
                @*<div class="ml-auto threeDotted">
                        <div class="dropdown sub-dropdown">
                            <button class="btn btn-link text-muted" type="button"
                                    id="dd1" data-toggle="dropdown" aria-haspopup="true"
                                    aria-expanded="false">
                                <i data-feather="more-vertical"></i>
                            </button>
                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dd1">
                                <a class="dropdown-item" href="javascript:void(0)" ng-click="DownloadLoginHis()">Download</a>
                                <a class="dropdown-item" href="javascript:void(0)">Email</a>
                            </div>
                        </div>
                    </div>*@
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Sr No.</th>
                            <th>Enquiry No</th>
                            <th>CompanyName</th>
                            <th>MotorType</th>
                            <th>PolicyType</th>
                            <th>Request Time</th>
                            <th>Request</th>
                            <th>Response Time</th>
                            <th>Response</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="item in LoadReport">
                            <td>{{$index+1}}</td>
                            <td>{{item.enquiryNo}}</td>
                            <td>{{item.companyName}}</td>
                            <td>{{item.motorType}}</td>
                            <td>{{item.policyType}}</td>
                            <td>{{item.requestTime}}</td>
                            <td><input type="text" value="{{item.request}}" class="form-control" /></td>
                            <td><input type="text" value="{{item.response}}" class="form-control" /></td>
                            <td>{{item.responseTime}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>