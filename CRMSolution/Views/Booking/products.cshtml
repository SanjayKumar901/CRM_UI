
@{
    ViewData["Title"] = "products";
}
<script src="https://centralapi.stariglocal.com/JsMaster/CRMJS/js/Booking/Bookingprivilege.js"></script>
<div class="page-wrapper ng-scope open_Main" ng-controller="Bookingprivilege" style="display: block;">
    <div class="container">
        <section class="mngUserfirst">
            <div class="row">
                <div class="col-md-6">
                    <h3>Products</h3>
                </div>
            </div>
        </section>
        <section class="mngUserSecond">
            <div class="row">
                <div class="col-md-6">
                    <div class="manageUserFLT">
                        <div class="row">
                            <div class="col-md-4">
                                <div class="contnt-div">
                                    <div ng-repeat="item in products">
                                        <a href="{{currentDomain}}{{item.url}}?user={{Token}}">{{item.privilegeName}}</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </div>
</div>