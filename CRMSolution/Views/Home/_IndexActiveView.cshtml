<!-- Start Lead Section -->
<div class="leadDetlsTable margin15_T_B" ng-show="IsLead">
    <div class="row">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="tableHeadRow">
                        <div class="row">
                            <div class="col-md-3 PadRight">
                                <h4 class="card-title">Lead Details #<span>{{leadCounter}}</span></h4>
                            </div>
                            <div class="col-md-3 PadRight">
                                <div class="pull-left">
                                    <div class="input-wrap HomeSearchBox">
                                        <input type="text" ng-model="SearchLead"
                                               ng-change="SearchLeadData(SearchLead)" placeholder="Search..." class="textdemo" />
                                        <i class="fa fa-search float-right"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 PadRight">
                                <div class="pull-left">
                                    <select class="theFilters" ng-model="Product"
                                            ng-change="SelectProduct(Product)">
                                        <option value="All">All</option>
                                        <option value="Car">Private Car</option>
                                        <option value="Two">Two Wheeler</option>
                                        <option value="Car-E">Private Car Break-In Cases</option>
                                        <option value="Two-E">Two Wheeler Break-In Cases</option>
                                        <option value="GCV">GCV</option>
                                        <option value="PCV">PCV</option>
                                        <option value="TL">Term Life</option>
                                        <option value="HLT">Health</option>
                                        <option value="TRV">Travel</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-2 PadRight">
                                <div class="pull-left">
                                    <select class="theFilters" ng-model="leadStatusId" ng-change="leadsChange(leadStatusId)">
                                        <option value="">Lead Status</option>
                                        <option value="Quotation Success">Quotation</option>
                                        <option value="Proposal Success">Proposal</option>
                                        <option value="Payment Success">Success</option>
                                        <option value="Payment Gateway">Payment Gateway</option>
                                        <option value="Payment Fail">Payment Fail</option>
                                    </select>
                                </div>
                            </div>

                            <div class="col-md-2 PadRight text-right">
                                <div id="reportrange" ng-click="getDateRangeValue('LeadReport')" class="pull-right theFilters theFiltersClndr">
                                    <span class="DateRangeShow"></span>
                                    <i class="fas fa-calendar-alt"></i><b class="caret"></b>
                                </div>
                                <div class="ml-auto threeDotted">
                                    <div class="dropdown sub-dropdown">
                                        <button class="btn btn-link text-muted" type="button"
                                                id="dd1" data-toggle="dropdown" aria-haspopup="true"
                                                aria-expanded="false">
                                            <i data-feather="more-vertical"></i>
                                        </button>
                                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dd1">
                                            <a class="dropdown-item" href="javascript:void(0)" ng-click="DownloadMotorReport()">Download</a>
                                            <a class="dropdown-item" href="javascript:void(0)">Email</a>

                                        </div>
                                    </div>
                                </div>

                                <!--Chart Implemenation, Date: 2021/08/06-->
                                <div class="col-md-1">
                                    <span ng-click="chartclick()" data-toggle="modal" data-target="#exampleModal"><i class="fa fa-eye"></i> Chart</span>
                                </div>

                                <div class="modal fade ProductChart" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Lead Details</h5>
                                                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="closeModel2">
                                                    <span aria-hidden="true">&times;</span>
                                                </button>
                                            </div>
                                            <div class="modal-body">
                                                <div class="MainChart">
                                                    <ul>
                                                        <li ng-repeat="item in chartList track by $index">
                                                            <input type="checkbox" ng-model="chartList[$index].selected" ng-change="chartItemChecked($index)" />
                                                            <span class="{{item.class}}"></span>
                                                            <p>{{item.name}}</p>
                                                        </li>
                                                    </ul>
                                                    <canvas id="myChart"></canvas>
                                                </div>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary" data-dismiss="modal" id="closeModel2">Close</button>
                                                @*<button type="button" class="btn btn-primary">Send message</button>*@
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--End Chart Implemenation, Date: 2021/08/06-->

                            </div>
                        </div>
                    </div>
                    <div class="tableContainer">
                        <span class="Tableloader"></span>

                        <table class="table table-bordered table-light" style="overflow: auto;height: 350px;">
                            <thead>
                                <tr>
                                    <th class="serial">Lead#</th>
                                    <th>Type</th>
                                    <th>Contact</th>
                                    <th>User</th>
                                    <th>Role</th>
                                    @*<th>Region</th>*@
                                    <th>Source</th>
                                    <th>Date</th>
                                    <th>Status</th>
                                    <th>Lead From</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="lead in LeadDetailGrid">
                                    <td><a href="javascript:void(0)" ng-click="GetEnquiryInfo(lead.enqNo,lead.policyStatus,lead.enquirytype)">{{lead.enqNo}}</a></td>
                                    <td>{{lead.enquirytype}}</td>
                                    <td>{{lead.mobileno}}</td>
                                    <td>{{lead.userName}}</td>
                                    <td>{{lead.roleName}}</td>
                                    @*<td>{{lead.region}}</td>*@
                                    <td>{{lead.leadsource}}</td>
                                    <td>{{lead.enquirydate}}</td>
                                    <td>{{lead.policyStatus}}</td>
                                    <td ng-init="opt=lead.macid==null?'Web':lead.macid">{{opt}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
<!-- End Lead Section -->
