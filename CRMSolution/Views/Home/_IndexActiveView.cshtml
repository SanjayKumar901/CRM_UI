                        <div class="row">
                            <div class="col-md-4 PadRight">
                                <h4 class="card-title">Users Birthday #{{BirthdayUserList.length}}</h4>
                            </div>
                            <div class="col-md-4 PadRight">
                                <input type="button" value="Send Wishes" ng-click="ShowWishForm()" />
                            </div>
                        </div>
                    </div>
                    <div class="tableContainer">
                        <span class="Tableloader"></span>
                        <table id="tblBirthday" class="table table-bordered table-light" style="overflow: auto;height: 350px;">
                            <thead>
                                <tr>
                                    <th>Select</th>
                                    <th>User Name</th>
                                    <th>Role</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr ng-repeat="lead in BirthdayUserList">
                                    <td><input type="checkbox" value="{{lead.userID}}" /></td>
                                    <td>{{lead.userName}}</td>
                                    <td>{{lead.roleName}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <!-- Start END BirthdayUserList Section -->
    </div>
</div>

<!--Birthday Notification Start-->
<div class="container" ng-show="BirthdayPopup">
    <div class="Createdmodal">
        <div class="modal-dialog BirthdayBox">
            <div class="modal-content">
                <input type="button" value="Close" ng-click="CLoseWishesBirth()" />
                <div class="modal-body">
                    <div class="BirthdayBoxTop">
                        <i class="fas fa-birthday-cake"></i>
                        <h3>Share your Wishes!</h3>
                    </div>

                    <div class="Birthform">
                        <form ng-submit="SHareBirthdayMessage()">
                            <textarea rows="3" class="form-control" ng-model="BirthdayMessage" maxlength="100" placeholder="Massage"></textarea>
                            <button class="btn birthBtn">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--Birthday Notification END-->

