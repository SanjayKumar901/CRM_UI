

<section class="mngUserSecond">
    <div class="row">
        <div class="col-md-8">
            <div class="manageUserFLT">
                <div class="row">
                    <div class="col-md-3">
                        <div class="contnt-div">
                            <span class="inquiry-text">Total NoPS</span>
                            <span id="totalUser">{{Math.round(TotalGCVNop)}}</span>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="contnt-div">
                            <span class="inquiry-text">Total Business</span>
                            <span id="totalUser">{{Math.round(TotalGCVBusiness)}}</span>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="contnt-div">
                            <span class="inquiry-text">Today Nops</span>
                            <span id="totalUser">{{Math.round(TodayGCVNop)}}</span>
                        </div>
                    </div>
                    <div class="col-md-3">
                        <div class="contnt-div">
                            <span class="inquiry-text">Today Business</span>
                            <span id="totalUser">{{Math.round(TodayGCVBusiness)}}</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-4 pull-right">
            <div class="manageUserFLT">
                <div class="alct-data3">
                    <select class="theFilters ng-pristine ng-valid ng-empty ng-touched" ng-model="GCVPCVBusinessFilter"
                            ng-change="FilterGCVPC(GCVPCVBusinessFilter)">
                        <option value="">Select Filter</option>
                        <option value="All">All</option>
                        <option value="GCV">GCV</option>
                        <option value="PCV">PCV</option>
                    </select>
                </div>


            </div>
        </div>
    </div>
</section>
<section class="project-tab">
    <div class="leadDetlsTable margin15_T_B">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-body">
                        <div class="tableHeadRow">
                            <div class="row">
                               
                                <div class="col-md-12 PadRight text-right">
                                    <div class="row">
                                        <div class="col-md-3 text-left BR_ShowingData">
                                            <span>Showing NOP : </span>
                                            <span>{{GCVPCVBusinessReport.length}}</span>
                                        </div>
                                        <div class="col-md-3 text-left BR_ShowingData">
                                            <span>Showing Total Business : </span>
                                            <span>{{sumbusiness}}</span>
                                        </div>
                                        <div class="col-md-4 text-right">

                                            <div class="input-wrap">
                                                <input type="text" name="Search" ng-model="FilterGCVModal"
                                                       ng-change="filterGCVBusiness(FilterGCVModal)" class="textdemo"
                                                       placeholder="Customer Name, Registration & Policy No."> <i class="fa fa-search"></i>
                                                <label class="demo-label" for="Search"></label>
                                            </div>

                                        </div>
                                        <div class="col-md-2">
                                            <div class="theFilters theFiltersClndr reportrange">
                                                <span class="DateRangeShow BR"></span>
                                                <i class="fa fa-calendar"></i><b class="caret"></b>
                                            </div>
                                            <div class="ml-auto threeDotted">
                                                <div class="dropdown sub-dropdown">
                                                    <button class="btn btn-link text-muted dropdown-toggle" type="button" id="dd1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                                        <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-more-vertical"><circle cx="12" cy="12" r="1"></circle><circle cx="12" cy="5" r="1"></circle><circle cx="12" cy="19" r="1"></circle></svg>
                                                    </button>
                                                    <div ng-show="IsDownload" class="dropdown-menu dropdown-menu-right" aria-labelledby="dd1" x-placement="bottom-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(-134px, 27px, 0px);">
                                                        <a class="dropdown-item" ng-click="DownloadMotorReport()"
                                                           href="javascript:void(0)">
                                                            Download
                                                        </a>
                                                        <a class="dropdown-item" href="javascript:void(0)">Email</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tableContainer">
                            <span class="Tableloader"></span>
                            <table class="table table-bordered MainTable table-light" s>
                                <thead>
                                    <tr>
                                        <th>Contact</th>
                                        <th>Insurer</th>
                                        <th>User Name</th>
                                        <th>User Role</th>
                                        <th>Customer</th>
                                        <th>Vehicle Type</th>
                                        <th>Vehicle</th>
                                        <th>Variant</th>
                                        <th>Registration#</th>
                                        <th>Policy#</th>
                                        <th>Amount</th>
                                        <th>Date</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="item in GCVPCVBusinessReport">
                                        <td ng-init="sumbusiness=sumbusiness+item.paidAmount"><a href="javascript:void(0)" ng-click="ShowDetails('CAR',item.enquiryNo)">{{item.mobileNo}}</a></td>
                                        <td>{{item.companyName}}</td>
                                        <td>{{item.userName}}</td>
                                        <td>{{item.userRole}}</td>
                                        <td>{{item.name}}</td>
                                        <td>{{item.motorType}}</td>
                                        <td>{{item.vehicleName}}</td>
                                        <td>{{item.variantName}}</td>
                                        <td>{{item.vehicleNo}}</td>
                                        <td><a href="javascript:void(0)" ng-click="GetPolicyPDF(item,'Motor')">{{item.policyNo}}</a></td>
                                        <td>{{item.paidAmount}}</td>
                                        <td>{{item.closeDate}}</td>
                                        <td>{{item.macid}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>