
@{
    ViewData["Title"] = "MissingPolicy";
}
<script src="https://centralapi.stariglocal.com/JsMaster/CRMJS/js/Business/MissingPolicy.js"></script>
<div class="page-wrapper mappedUser open_Main" ng-controller="missingpolicy">
    <div class="container">
        <section class="mngUserfirst">
            <div class="row">
                <div class="col-md-6">
                    <h3>Get upload missing policy</h3>
                </div>
            </div>
        </section>
        <div class="row">
            <div class="col-md-12 text-center">
                <section class="mngUserfirst mngUserfirstMapping">
                    <div class="row">

                        <div class="col-md-12">
                            <div class="manageUserFLT">
                                <div class="row getResponseMain">
                                    <div class="col-md-2 PadRight">
                                        <div class="">
                                            <select class="theFiltersBulk theFilters" ng-model="Product" 
                                                    ng-change="ChangeProduct(Product)">
                                                <option value="motor">Motor</option>>
                                                <option value="health">Health</option>>
                                            </select>
                                        </div>
                                    </div>

                                    <div class="col-md-2 PadRight">
                                        <div class="">
                                            <select class="theFiltersBulk theFilters" ng-model="Company"
                                                    ng-options="item.companyName for item in CompanyList">
                                                <option value="">Select</option>>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-8 createUserBox GetRequest_Response02 text-left">
                                        <form>
                                            <div class="input-wrap">
                                                <input type="text" ng-model="PolicyNumber" placeholder=" " class="textdemo" />
                                                <label class="demo-label" for="Mobile">Policy Number</label>
                                            </div>
                                            <div class="input-wrap" ng-show="Product=='motor'">
                                                <input type="text" ng-model="EngineNo" placeholder=" " class="textdemo" />
                                                <label class="demo-label" for="Mobile">Engine Number</label>
                                            </div>
                                            <div class="input-wrap">
                                                <input type="file" name="Email" id="fileupload" class="textdemo">
                                            </div>
                                            <div class="input-wrap">
                                                <input type="button" value="Create" ng-click="fileNameChanged()" />
                                            </div>
                                        </form>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </section>
                <div class="leadDetlsTable margin15_T_B">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="card">
                                <div class="card-body">
                                    <div class="tableHeadRow">
                                        <div class="row">
                                            <div class="col-md-6 PadRight">
                                                <h4 class="card-title">No of Record : <span class="ng-binding">0</span></h4> <h4 class="card-title">Total Business : <span class="ng-binding">0</span></h4>
                                            </div>
                                            <div class="col-md-6 PadRight text-right">
                                                <div class="row">
                                                    <div class="col-md-8 text-right">
                                                        <div class="input-wrap">
                                                            <input type="text" name="Search" class="textdemo" placeholder="Search..."> <i class="fa fa-search"></i>
                                                            <label class="demo-label" for="Search"></label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div id="reportrange" ng-click="getDateRangeValue('LeadReport')" class="theFilters theFiltersClndr">
                                                            <span class="DateRangeShow BR"></span>
                                                            <i class="fa fa-calendar"></i><b class="caret"></b>
                                                        </div>
                                                        <div class="ml-auto threeDotted">
                                                            <div class="dropdown sub-dropdown">
                                                                <button class="btn btn-link text-muted dropdown-toggle" type="button" id="dd1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
                                                                    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-more-vertical"><circle cx="12" cy="12" r="1"></circle><circle cx="12" cy="5" r="1"></circle><circle cx="12" cy="19" r="1"></circle></svg>
                                                                </button>
                                                                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dd1" x-placement="bottom-end" style="position: absolute; will-change: transform; top: 0px; left: 0px; transform: translate3d(-134px, 27px, 0px);">
                                                                    <a class="dropdown-item" href="javascript:void(0)">Download</a>
                                                                    <a class="dropdown-item" href="javascript:void(0)">Email</a>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <table class="table table-bordered MainTable text-left table-light">
                                        <thead>
                                            <tr>
                                                <th>Enquiry ID</th>
                                                <th>Enquiry Status</th>
                                                <th>Policy#</th>
                                                <th>Insurer Name</th>
                                                <th>Vehicle#</th>
                                                <th>Chassis#</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr role="row" class="odd" ng-repeat="item in Policies">
                                                <td>{{item.id}}</td>
                                                <td>{{item.enquiryStatus}}</td>
                                                <td>{{item.policyNo}}</td>
                                                <td>{{item.companyName}}</td>
                                                <td>{{item.vehicleNo}}</td>
                                                <td>{{item.chasisNo}}</td>                                               
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div id="full-width-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="fullWidthModalLabel" aria-hidden="true">
        <div class="modal-dialog modal-full-width">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title" id="fullWidthModalLabel">Modal Heading</h4>
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                        ×
                    </button>
                </div>
                <div class="modal-body">
                    <div class="leadDetlsTable margin15_T_B">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="card">
                                    <div class="card-body">
                                        <div class="tableHeadRow">
                                            <div class="row">
                                                <div class="col-md-2">
                                                    <h4 class="card-title">Lead Details #<span class="ng-binding"></span></h4>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="">
                                                        <select id="leadStatusId" class="theFiltersBulk theFilters">
                                                            <option value="All" selected="selected">Region Wise</option>
                                                            <option value="Quotation Success">Delhi</option>
                                                            <option value="Proposal Success">Mumbai</option>
                                                            <option value="Payment Success">UP</option>
                                                            <option value="Payment Gateway">Rajasthan</option>
                                                            <option value="Payment Fail">MP</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="">
                                                        <select id="leadStatusId" class="theFiltersBulk theFilters ng-pristine ng-untouched ng-valid ng-empty" ng-model="leadStatusId" ng-change="leadsChange(leadStatusId)">
                                                            <option value="" selected="selected">Product Wise</option>
                                                            <option value="Quotation Success">4 Wheeler</option>
                                                            <option value="Proposal Success">4 Wheeler-TP</option>
                                                            <option value="Payment Success">2 Wheeler</option>
                                                            <option value="Payment Gateway">2 Wheeler-TP</option>
                                                            <option value="Payment Gateway">OD</option>
                                                            <option value="Payment Gateway">Taxi</option>
                                                            <option value="Payment Gateway">GCV</option>
                                                            <option value="Payment Fail">Health</option>
                                                            <option value="Payment Fail">Travel</option>
                                                            <option value="Payment Fail">Life</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-2">
                                                    <div class="">
                                                        <select id="leadStatusId" class="theFiltersBulk theFilters">
                                                            <option value="All" selected="selected">Lead Status</option>
                                                            <option value="Quotation Success">Quotation</option>
                                                            <option value="Proposal Success">Proposal</option>
                                                            <option value="Payment Success">Success</option>
                                                            <option value="Payment Gateway">Payment Gateway</option>
                                                            <option value="Payment Fail">Fail</option>
                                                        </select>
                                                    </div>
                                                </div>

                                                <div class="col-md-4">
                                                    <div id="reportrange" class="pull-right theFiltersBulk theFilters theFiltersBulk theFiltersClndr">
                                                        <i class="glyphicon glyphicon-calendar fa fa-calendar"></i>
                                                        <span>2020-04-29 - 2020-04-29</span> <b class="caret"></b>
                                                    </div>
                                                    <div class="ml-auto threeDotted">
                                                        <div class="dropdown sub-dropdown">
                                                            <button class="btn btn-link text-muted dropdown-toggle" type="button" id="dd1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                                                <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" class="feather feather-more-vertical"><circle cx="12" cy="12" r="1"></circle><circle cx="12" cy="5" r="1"></circle><circle cx="12" cy="19" r="1"></circle></svg>
                                                            </button>
                                                            <div class="dropdown-menu dropdown-menu-right" aria-labelledby="dd1">
                                                                <a class="dropdown-item" href="http://crmapi.stariengineering.com/crm/Home/Index#">Download</a>
                                                                <a class="dropdown-item" href="http://crmapi.stariengineering.com/crm/Home/Index#">Email</a>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>
                                        <table class="table table-bordered table-responsive table-light" style="overflow: auto;height: 350px;">
                                            <thead>
                                                <tr>
                                                    <th class="serial">Lead#</th>
                                                    <th>Type</th>
                                                    <th>Contact</th>
                                                    <th>User</th>
                                                    <th>Role</th>
                                                    <th>Region</th>
                                                    <th>Source</th>
                                                    <th>Date</th>
                                                    <th>Status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                <!-- ngRepeat: lead in LeadDetailGrid -->
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-light" data-dismiss="modal">
                        Close
                    </button>
                    <button type="button" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div><!-- /.modal-dialog -->
    </div>

</div>